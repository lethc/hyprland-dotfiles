[
    {
        "layer": "top",
        "position": "top",
        "mod": "dock",
        "exclusive": true,
        "gtk-layer-shell": true,
        "margin-bottom": -1,
        "passthrough": false,
        "height": 24,
        "spacing": 5,
        "modules-left": [
            "custom/launcher",
            // "custom/os_button",
            // "hyprland/window",
            "hyprland/workspaces",
            "custom/texttwo",
            "custom/textthree",
            "custom/textfour",
            "custom/textfive"
        ],
        "modules-center": [
            // "sway/window"
            "custom/reminder2",
            "custom/separator",
            "custom/reminder"
        ],
        "modules-right": [
            // "mpd",
            // "temperature",
            "custom/sunset",
            "custom/monitor",
            "cpu",
            "memory",
            "network",
            "pulseaudio",
            "backlight",
            // "keyboard-state",
            "battery",
            "battery#bat2",
            "tray",
            "clock"
        ],
        "hyprland/window": {
            "format": "{class}",
            "max-length": 20,
            "rewrite": {
                "^(?!.*\\S).*": "Finder"
            }
        },
        "custom/launcher": {
            "format": "Û±óÜ",
            "on-click": "rofi -show drun -theme spotlight",
            "on-click-right": "pkill rofi",
            "tooltip": false
        },
        "hyprland/workspaces": {
            "icon-size": 32,
            "spacing": 16,
            "format": "{icon}",
            "all-outputs": false,
            "format-icons": {
                "A:Cy√æan": " Ôàö Cy√æan ",
                "B:Lƒ´√æan": " Û∞†≥ Lƒ´√æan ",
                "C:W«£pn": " Û∞Ñª W«£pn ",
                "D:W√¶l": " Û∞ßº W√¶l ",
                "E:ƒ†emynd": " Óª´ ƒ†emynd ",
                "F:ƒ†esiht": " Û∞°Æ ƒ†esiht ",
                "G:HƒÅm": " Ó¥• HƒÅm ",
                "H:Sƒãƒìawere": " Óéæ Sƒãƒìawere",
                "I:Mƒìce": " Û∞ûá Mƒìce ",
                "J:Frƒìo√æ": " ÓµÉ Frƒìo√æ"
            },
            "on-scroll-up": "~/.config/hypr/dots/.scripts/Hyprland/switchWorkspace up",
            "on-scroll-down": "~/.config/hypr/dots/.scripts/Hyprland/switchWorkspace down"
        },
        "custom/texttwo": {
            "exec": "echo 'File'",
            "interval": 60,
            "return-type": "plain",
            "on-click": "nautilus"
        },
        "custom/textthree": {
            "exec": "echo 'Edit'",
            "interval": 60,
            "return-type": "plain",
            "on-click": "gimp"
        },
        "custom/textfour": {
            "exec": "echo 'View'",
            "interval": 60,
            "return-type": "plain"
        },
        "custom/textfive": {
            "exec": "echo 'Help'",
            "interval": 60,
            "return-type": "plain",
            "on-click": "gio open https://github.com/kamlendras/waybar/issues/new"
        },
        "custom/separator": {
            "format": " ÓòÇ",
            "tooltip": false
        },
        "custom/reminder": {
            "format": "{}",
            "exec": "~/.config/waybar/bin/reminder",
            "return-type": "json",
            "interval": 10800,
            "signal": 8
        },
        "custom/reminder2": {
            "format": "{}",
            "exec": "~/.config/waybar/bin/reminder2",
            "return-type": "json",
            "interval": 10800,
            "signal": 8
        },
        // "sway/window": {
        //     "max-length": 50
        // },
        "custom/sunset": {
            "format": "Û∞Éû ",
            // "max-length": 10,
            "on-click": "~/.config/waybar/bin/control-hyprsunset enable", // Enable blue-light-filter
            "on-click-right": "~/.config/waybar/bin/control-hyprsunset disable", // Disable blue-light-filter
            "on-scroll-up": "~/.config/waybar/bin/control-hyprsunset increase",
            "on-scroll-down": "~/.config/waybar/bin/control-hyprsunset decrease",
            // "tooltip": false
            "tooltip-format": "Control the Brightness of the Screen"
        },
        "custom/monitor": {
            "format": " Û∞®ö monitor",
            "on-click": "~/.config/hypr/dots/.scripts/Hyprland/externalMonitor",
            "on-click-right": "hyprctl reload",
            "tooltip-format": "External Monitor"
        },
        "cpu": {
            "interval": 5,
            "format": "Ôãõ {usage}%"
            // "max-length": 10
        },
        "memory": {
            // "interval": 10,
            "format": "ÓøÖ {}%",
            // "max-length": 10,
            "tooltip": true,
            "tooltip-format": "RAM - {used:0.1f}GiB used (ÓøÖ {percentage}%)"
        },
        "network": {
            "format-wifi": "{essid} ({signalStrength}%) Ôá´",
            "format-ethernet": "Û∞àÄ {ifname}",
            "format-linked": "Û∞àÅ {ifname} (No IP)",
            "format-disconnected": "Disconnected Û∞ñ™ {ifname}",
            // "format-alt": "Ôûñ {ifname}: {ipaddr}/{cidr}",
            "tooltip-format": "Û∞äó {ifname} via {gwaddr}",
            // "on-click-middle": "nm-connection-editor"
            "on-click": "nm-connection-editor"
        },
        "pulseaudio": {
            "scroll-step": 5,
            "format": "{icon}  {volume}%   {format_source}",
            "format-bluetooth": "Ôäî {icon} {volume}% {format_source}",
            "format-bluetooth-muted": "Ôö© Ôäî {icon} {format_source}",
            "format-muted": "Ôö©  {format_source}",
            "format-source": "ÔÑ∞ {volume}%",
            "format-source-muted": "ÔÑ±",
            "format-icons": {
                "default": [
                    "ÔÄ¶",
                    "ÔÄß",
                    "ÔÄ®"
                ]
            },
            "on-click": "pavucontrol",
            // "on-click-right": "volume mute",
            "on-click-right": "foot -a pw-top pw-top",
            // "on-scroll-up": "pactl set-sink-volume @DEFAULT_SINK@ +5%",
            // "on-scroll-down": "pactl set-sink-volume @DEFAULT_SINK@ -5%",
            "smooth-scrolling-threshold": 1
        },
        "backlight": {
            "format": "{icon} {percent}%",
            "format-icons": [
                "Óéç",
                "Óèì",
                "Óèë",
                "Óèè",
                "Óèé",
                "Óèç",
                "Óèä",
                "Óèà",
                "Óéõ"
            ]
        },
        // "keyboard-state": {
        //     "numlock": true,
        //     "capslock": true,
        //     "format": "{name} {icon}",
        //     "format-icons": {
        //         "locked": "ÔÄ£",
        //         "unlocked": "ÔÇú"
        //     }
        // },
        "battery": {
            "states": {
                "good": 95,
                "warning": 30,
                "critical": 15
            },
            "format": "{icon} {capacity}%",
            "format-charging": "Ôóß {capacity}%",
            // "format-full": "<span color='#82A55F'><b>{icon}</b></span>",
            "format-plugged": "Ôá¶ {capacity}%",
            "format-alt": "{icon} {time}",
            "format-icons": [
                "ÔâÑ",
                "ÔâÉ",
                "ÔâÇ",
                "ÔâÅ",
                "ÔâÄ"
            ]
        },
        "battery#bat2": {
            "bat": "BAT2"
        },
        "tray": {
            // "icon-size": 18,
            "spacing": 10
        },
        "clock": {
            "format": "{:%A %B %d %H:%M }",
            "tooltip-format": "<tt><small>{calendar}</small></tt>",
            "calendar": {
                "mode": "year",
                "mode-mon-col": 3,
                "weeks-pos": "right",
                "on-scroll": 1,
                "on-click-right": "mode",
                "format": {
                    "months": "<span color='#e7c787'><b>{}</b></span>",
                    "days": "<span color='#fca2aa'>{}</span>",
                    "weeks": "<span color='#98c379'><b>W{}</b></span>",
                    "weekdays": "<span color='#61afef'><b>{}</b></span>",
                    "today": "<span color='#e06c75'><b><u>{}</u></b></span>"
                }
            },
            "actions": {
                "on-click-right": "mode",
                "on-click-forward": "tz_up",
                "on-click-backward": "tz_down",
                "on-scroll-up": "shift_up",
                "on-scroll-down": "shift_down"
            }
        }
    }
