[
    {
        "layer": "top",
        "position": "top",
        "mod": "dock",
        "exclusive": true,
        "gtk-layer-shell": true,
        "margin-bottom": -1,
        "passthrough": false,
        "height": 24,
        // "spacing": 1,
        "modules-left": [
            "custom/launcher",
            // "custom/os_button",
            // "hyprland/window",
            "hyprland/workspaces"
            // "wlr/taskbar"
            // "custom/texttwo",
            // "custom/textthree",
            // "custom/textfour",
            // "custom/textfive"
        ],
        "modules-center": [
            "custom/reminder2",
            "custom/separator",
            "custom/reminder"
        ],
        "modules-right": [
            // "mpd",
            // "temperature",
            "tray",
            "custom/monitor",
            "custom/sunset",
            "cpu",
            "memory",
            "disk",
            "network",
            "custom/notifications",
            "pulseaudio",
            // "backlight",
            // "keyboard-state",
            "battery",
            "clock"
        ],
        "hyprland/window": {
            "format": "{class}",
            "max-length": 20,
            "rewrite": {
                "^(?!.*\\S).*": "Finder"
            }
        },
        "custom/launcher": {
            "format": "󱗆",
            "on-click": "rofi -show drun -theme spotlight-dark",
            "on-click-right": "pkill rofi",
            "tooltip": false
        },
        "custom/notifications": {
            "format": "󰂞",
            "on-click": "swaync-client -t -sw",
            "on-click-right": "swaync-client -t -sw",
            "tooltip": true,
            "tooltip-format": "Notifications"
        },
        "hyprland/workspaces": {
            "all-outputs": false,
            "warp-on-scroll": false,
            "format": "{icon}{windows}",
            // "icon-size": 32,
            // "spacing": 16,
            // "disable-scroll": true,
            "on-scroll-up": "~/.config/hypr/dots/.scripts/Hyprland/switchWorkspace up",
            "on-scroll-down": "~/.config/hypr/dots/.scripts/Hyprland/switchWorkspace down",
            "format-icons": {
                "A:Cyþan": "  Cyþan ",
                "B:Līþan": " 󰠳 Līþan ",
                "C:Wǣpn": " 󰄻 Wǣpn ",
                "D:Wæl": " 󰧼 Wæl ",
                "E:Ġemynd": "  Ġemynd ",
                "F:Ġesiht": " 󰡮 Ġesiht ",
                "G:Hām": "  Hām ",
                "H:Sċēawere": "  Sċēawere",
                "I:Mēce": " 󰞇 Mēce ",
                "J:Frēoþ": "  Frēoþ"
            },
            "workspace-taskbar": {
                "enable": true,
                "update-active-window": true,
                "format": "{icon}",
                "icon-size": 18,
                "on-click-window": "~/.config/waybar/bin/focus-window.sh {address} {button}"
            }
        },
        "custom/texttwo": {
            "exec": "echo 'File'",
            "interval": 60,
            "return-type": "plain",
            "on-click": "nautilus"
        },
        "custom/textthree": {
            "exec": "echo 'Edit'",
            "interval": 60,
            "return-type": "plain",
            "on-click": "gimp"
        },
        "custom/textfour": {
            "exec": "echo 'View'",
            "interval": 60,
            "return-type": "plain"
        },
        "custom/textfive": {
            "exec": "echo 'Help'",
            "interval": 60,
            "return-type": "plain",
            "on-click": "gio open https://github.com/kamlendras/waybar/issues/new"
        },
        "custom/separator": {
            "format": " → ",
            "tooltip": false
        },
        "custom/reminder": {
            "format": "{}",
            "exec": "~/Home/Scripts/Rofi/reminder",
            "return-type": "json",
            "interval": 10800,
            "signal": 8
        },
        "custom/reminder2": {
            "format": "{}",
            "exec": "~/Home/Scripts/Rofi/reminder2",
            "return-type": "json",
            "interval": 10800,
            "signal": 8
        },
        // "sway/window": {
        //     "max-length": 50
        // },
        "custom/sunset": {
            "format": "󰃞 Ξ",
            "on-click": "~/.config/waybar/bin/control-hyprsunset enable", // Enable blue-light-filter
            "on-click-right": "~/.config/waybar/bin/control-hyprsunset disable", // Disable blue-light-filter
            "on-scroll-up": "~/.config/waybar/bin/control-hyprsunset increase",
            "on-scroll-down": "~/.config/waybar/bin/control-hyprsunset decrease",
            // "tooltip": false
            "tooltip-format": "Control the Brightness of the Screen"
        },
        "custom/monitor": {
            // "format": " 󰨚 monitor",
            "format": "󰍺 Μ",
            "on-click": "~/.config/hypr/dots/.scripts/Hyprland/externalMonitor",
            "on-click-right": "hyprctl reload",
            "tooltip-format": "External Monitor"
        },
        "cpu": {
            "interval": 5,
            "format": " {usage}"
            // "max-length": 10
        },
        "memory": {
            // "interval": 10,
            "format": " {}",
            // "max-length": 10,
            "tooltip": true,
            "tooltip-format": "RAM - {used:0.1f}GiB used ( {percentage}%)"
        },
        "disk": {
            "interval": 30,
            "format": "󰋊 {}",
            "path": "/",
            "tooltip": true,
            "unit": "GB",
            "tooltip-format": "Available {free} of {total} (󰋊 {percentage_used}%)"
        },
        "network": {
            "format-wifi": " ({signalStrength})",
            "format-ethernet": "󰈀 {ifname}",
            "format-linked": "󰈁 {ifname} (No IP)",
            "format-disconnected": "Disconnected 󰖪 {ifname}",
            // "format-alt": " {ifname}: {ipaddr}/{cidr}",
            "tooltip-format": "{essid} ({signalStrength}%) | 󰊗 {ifname} via {gwaddr}",
            // "on-click-middle": "nm-connection-editor"
            "on-click": "nm-connection-editor"
        },
        "pulseaudio": {
            "scroll-step": 5,
            "format": "{icon}  {volume}",
            "format-bluetooth": " {icon} {volume}% {format_source}",
            "format-bluetooth-muted": "  {icon} {format_source}",
            "format-muted": "  {format_source}",
            "format-source": " {volume}%",
            "format-source-muted": "",
            "tooltip-format": "Microphone: {format_source}",
            "format-icons": {
                "default": [
                    "",
                    "",
                    ""
                ]
            },
            "on-click": "pavucontrol",
            // "on-click-right": "volume mute",
            "on-click-right": "foot -a pw-top pw-top",
            // "on-scroll-up": "pactl set-sink-volume @DEFAULT_SINK@ +5%",
            // "on-scroll-down": "pactl set-sink-volume @DEFAULT_SINK@ -5%",
            "smooth-scrolling-threshold": 1
        },
        "backlight": {
            "format": "{icon} {percent}%",
            "format-icons": [
                "",
                "",
                "",
                "",
                "",
                "",
                "",
                "",
                ""
            ]
        },
        // "keyboard-state": {
        //     "numlock": true,
        //     "capslock": true,
        //     "format": "{name} {icon}",
        //     "format-icons": {
        //         "locked": "",
        //         "unlocked": ""
        //     }
        // },
        "battery": {
            "states": {
                "good": 95,
                "warning": 30,
                "critical": 15
            },
            "format": "{icon} {capacity}",
            "format-charging": " {capacity}",
            // "format-full": "<span color='#82A55F'><b>{icon}</b></span>",
            "format-plugged": " {capacity}",
            "format-alt": "{icon} {time}",
            "format-icons": [
                "",
                "",
                "",
                "",
                ""
            ]
        },
        "tray": {
            "icon-size": 14,
            "spacing": 6
        },
        "clock": {
            "format": "{:%a %d %b %H:%M }",
            "tooltip-format": "<tt><small>{calendar}</small></tt>",
            "calendar": {
                "mode": "year",
                "mode-mon-col": 3,
                "weeks-pos": "right",
                "on-scroll": 1,
                "on-click-right": "mode",
                "format": {
                    "months": "<span color='#a998de'><b>{}</b></span>",
                    "days": "<span color='#e2dbf4'>{}</span>",
                    "weeks": "<span color='#7f2858'><b>W{}</b></span>",
                    "weekdays": "<span color='#61afef'><b>{}</b></span>",
                    "today": "<span color='#c85163'><b><u>{}</u></b></span>"
                }
            },
            "actions": {
                "on-click-right": "mode",
                "on-click-forward": "tz_up",
                "on-click-backward": "tz_down",
                "on-scroll-up": "shift_up",
                "on-scroll-down": "shift_down"
            }
        },
        "wlr/taskbar": {
            // "format": "{icon}",
            "all-outputs": false,
            "format": "{icon} {title:.15}",
            "icon-size": 20,
            // "spacing": 3,
            "on-click-middle": "close",
            "tooltip-format": "{title}",
            "ignore-list": [],
            "on-click": "activate"
        }
    }
